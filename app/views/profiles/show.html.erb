    <div class="chat-window-container"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-6">

                <!--Friends -->
                <div class="panel panel-default">
                    <div class="panel-heading panel-heading-gray">
                        <a href="#" class="btn btn-white btn-xs pull-right"><i class="fa fa-pencil"></i></a>
                        <i class="fa fa-info-circle"></i> About
                    </div>
                    <div class="panel-body">
                        <ul class="list-unstyled profile-about">

                            <li>
                                <div class="row">
                                    <div class="col-sm-4"><span class="text-muted">Name</span>
                                    </div>
                                    <div class="col-sm-8"><%=@profile.name%></div>
                                </div>
                            </li>

                            <li>
                                <div class="row">
                                    <div class="col-sm-4"><span class="text-muted">Family Name</span>
                                    </div>
                                    <div class="col-sm-8"><%=@profile.familyName%></div>
                                </div>
                            </li>
                            <li>
                                <div class="row">
                                    <div class="col-sm-4"><span class="text-muted">Date of Birth</span>
                                    </div>
                                    <div class="col-sm-8"><%=@profile.date_of_birth%></div>
                                </div>
                            </li>
                            <li>
                                <div class="row">
                                    <div class="col-sm-4"><span class="text-muted">Gender</span>
                                    </div>
                                    <div class="col-sm-8"><%=@profile.Gender%></div>
                                </div>
                            </li>
                            <li>
                                <div class="row">
                                    <div class="col-sm-4"><span class="text-muted">Lives in</span>
                                    </div>
                                    <div class="col-sm-8"><%=@profile.country%></div>
                                </div>
                            </li>
                        </ul>
                    <% if current_user.profile != @profile %>
                      <%= form_tag("/friendables/friend_request?id=#{@profile.user.id}" , :remote => true, :id => "request") do %>
                          <% if Friendable.where(:from_id => current_user.id,:to_id => @profile.user.id).blank? %>
                              <%= submit_tag 'follow', :class => 'btn btn-success' %>
                          <% else %>
                              <%= submit_tag 'unfollow', :class => 'btn btn-danger' %>
                          <% end %>
                      <% end %>
                    <% end %>


                    </div>
                </div>
            </div>
            <div class="col-md-6">

                <!--Friends -->
                <div class="panel panel-default">
                    <div class="panel-heading panel-heading-gray">
                        Following
                    </div>
                    <ul class="list-unstyled friends-list">
                      <% if not @followings.nil? %>
                          <% @followings.each do |fo| %>
                              <%# byebug %>
                              <li>
                                <% _user = User.find_by_id(fo.to_id) %>
                                <%= image_tag _user.profile.try(:avatar) if _user.profile.present? %>
                                <%= _user.profile.name%>
                              </li>
                          <% end %>
                          <%end%>
                    </ul>

                    <div class="clearfix"></div>
                  <div class="panel-heading panel-heading-gray">
                    Followers
                  </div>
                  <ul class="list-unstyled friends-list">
                    <% if not @followees.nil? %>
                        <% @followees.each do |fo| %>
                            <%# byebug %>
                            <li>
                              <% _user = User.find_by_id(fo.from_id) %>
                              <%= image_tag _user.profile.try(:avatar) if _user.profile.present? %>
                              <%= _user.profile.name%>
                            </li>
                        <% end %>
                    <%end%>

                  </ul>

                  <div class="clearfix"></div>
                </div>
            </div>

        </div>
