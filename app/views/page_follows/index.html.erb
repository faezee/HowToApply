<p id="notice"><%= notice %></p>

<h1>Listing Page Follows</h1>

<table>
  <thead>
    <tr>
      <th>Folowee type</th>
      <th>Folowee</th>
      <th>Folower type</th>
      <th>Folower</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_follows.each do |page_follow| %>
      <tr>
        <td><%= page_follow.folowee_type %></td>
        <td><%= page_follow.folowee_id %></td>
        <td><%= page_follow.folower_type %></td>
        <td><%= page_follow.folower_id %></td>
        <td><%= link_to 'Show', page_follow %></td>
        <td><%= link_to 'Edit', edit_page_follow_path(page_follow) %></td>
        <td><%= link_to 'Destroy', page_follow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

