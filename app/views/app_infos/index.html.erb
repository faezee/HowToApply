<p id="notice"><%= notice %></p>

<h1>Listing App Infos</h1>

<table>
  <thead>
    <tr>
      <th>Plan</th>
      <th>Profile</th>
      <th>Acc or rej</th>
      <th>Professer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_infos.each do |app_info| %>
      <tr>
        <td><%= app_info.plan %></td>
        <td><%= app_info.profile %></td>
        <td><%= app_info.acc_or_rej %></td>
        <td><%= app_info.professer %></td>
        <td><%= link_to 'Show', app_info %></td>
        <td><%= link_to 'Edit', edit_app_info_path(app_info) %></td>
        <td><%= link_to 'Destroy', app_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App info', new_app_info_path %>
